{
    "@id": "dtmi:com:example:TermoSensor;1",
    "@type": "Interface",
    "displayName": "Termo Sensor",
    "contents": [
        {
            "@type": "Property",
            "name": "name",
            "schema": "string",
            "writable": true,
            "displayName": "Name",
            "description": "Name configured by user"
        },
        {
            "@type": "Property",
            "name": "location",
            "schema": "string",
            "writable": true,
            "displayName": "Location",
            "description": "Location of the sensor specified by user"
        },
        {
            "@type": "Property",
            "name": "temperature",
            "schema": "double",
            "writable": false,
            "displayName": "Temperature",
            "description": "Current temperature in given location"
        },
        {
            "@type": "Property",
            "name": "status",
            "schema": {
                "@type": "Enum",
                "valueSchema": "string",
                "enumValues": [
                    {"name": "on", "displayName": "On", "enumValue": "on"},
                    {"name": "off", "displayName": "Off", "enumValue": "off"},
                    {"name": "na", "displayName": "N/A", "enumValue": "na"},
                    {"name": "partial", "displayName": "Partial", "enumValue": "partial"}
                ]
            },
            "writable": true,
            "displayName": "Status",
            "description": "Operational status"
        }
    ],
    "@context": "dtmi:dtdl:context;2"
}
